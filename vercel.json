{
	"version": 2,
	"builds": [{ "src": "src/server.ts", "use": "@vercel/node" }],
	"routes": [
	  { "src": "/api-docs", "dest": "src/index.ts" },
	  { "src": "/static/(.*)", "dest": "/static/$1" },
	  { "src": "/index.html", "dest": "/index.html" }, 
	  { "src": "/(.*)", "dest": "src/index.ts" }
	]
  }
  